{
  "name": "davitec/ab-downloads",
  "type": "typo3-cms-extension",
  "description": "Modern Downloads",
  "homepage": "https://matthies.de",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": "*"
  },
  "replace": {
    "ab_downloads": "self.version"
  },
  "autoload": {
	  "psr-4": {
		  "Davitec\\AbDownloads\\": "Classes"
	  },
    "classmap": [
      "pi1",
      "lib"
    ]
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ab_downloads"
    }
  }
}
